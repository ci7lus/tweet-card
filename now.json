{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.+)",
      "dest": "src/index.ts"
    }
  ],
  "builds": [
    {
      "src": "src/**/*.ts",
      "use": "@now/node@canary"
    },
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
